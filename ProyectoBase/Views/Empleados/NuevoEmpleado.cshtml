                async: false,
                data: '',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (respuesta) {
                    var total = 0;
                    if (respuesta.PersonasCursos != null) {
                        total += respuesta.PersonasCursos.length;
                    }

                    if (respuesta.PersonasDiplomados != null) {
                        total += respuesta.PersonasDiplomados.length;
                    }

                    if (respuesta.PersonasCertificaciones != null) {
                        total += respuesta.PersonasCertificaciones.length;
                    }

                    if (total > 0) {
                        CDC = true;
                    }
                },
                error: errores
            });
        }

        if (Estudios) {

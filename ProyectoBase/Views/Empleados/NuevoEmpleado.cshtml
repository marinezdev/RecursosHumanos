                                type: 'warning',
                                allow_dismiss: false,
                                animate: {
                                    enter: 'animated rollIn',
                                    exit: 'animated rollOut'
                                }
                            });
                        }
                    }).done(function (results) {
                        setTimeout(CerrarModal, 1000);
                    });;

                } else {
                    $.notify({
                        message: "Intentas cargar una imagen muy pesada: " + Tam + " MG" + " Límite permitido 10 MG",
                    }, {
                        type: 'warning',
                        allow_dismiss: false,

        display: none;
        z-index: 9999999;
    }


    .btn-danger {
        background-color: #ff5252;
        border-color: #ff5252;
        background: linear-gradient(to right, #b02750, #e91e63);
        color: #fff;
        cursor: pointer;
        -webkit-transition: all ease-in 0.3s;
        transition: all ease-in 0.3s;
    }

    .btn-warning {
        background-color: #ffe100;

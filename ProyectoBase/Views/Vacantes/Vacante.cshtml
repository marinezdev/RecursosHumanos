                                                                        {
                                                                            foreach (var item in ViewBag.Bitacora)
                                                                            {
                                                                                if (item.Cat_Operacion.Nombre == "PROSPECTO REGISTRADO" & item.Usuarios.Personas.Id == 1)
                                                                                {
